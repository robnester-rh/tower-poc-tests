node(){
//    checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/robnester-rh/tower-poc-tests']]])
    def configYaml = readYaml file: '${WORKSPACE}config.yml'
    echo configYaml

    infraProvision(configYaml)

    stage('get env vars host'){
        ansiColor('xterm') {
            echo "RHEL HOST: ${env.RHEL_HOST}"
            echo "RHEL HOST (no env): ${env.RHEL_HOST}"
            echo sh(returnStdout: true, script: 'env')
        }
    }
}

